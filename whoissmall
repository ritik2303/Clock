#include<stdio.h>
int main()
{
     int a,b,c;
     printf("enter the age:");
     scanf("%d%d%d",&a,&b,&c);
     if (a<b && a<c)
     {
         printf("x is youngest among the three");
     }
     else if(b<c && b<a)
     {
         printf("y is youngest among the three");
     }
     else
     {
         printf("z is youngest among the three");
     }
     return 0;
}     
